def function fibonacci with parameters(n) {
    if n == 0 {
        return
    }
    else {
        call fibonacci with parameters (n - 1)
    }
    print n
}

def function sum with parameters(n) {
    if n == 0 {
        return n
    }

    return n + call sum with parameters(n - 1)
}