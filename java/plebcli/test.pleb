def function fib with parameters(n) {

    if n <= 0 {
        return 0
    }
    if n == 1 {
        return 1
    }

    return call fib with parameters (n - 1) + call fib with parameters (n - 2)
}

def function sum_of_0_to_n_powered_to_k with parameters(n, k) {
    if n == 0 {
        return n
    }

    def var result = n ^ k
    return result + call sum_of_0_to_n_powered_to_k with parameters(n - 1, k)
}

def function max with parameters(a, b) {
    if a > b {
        return a
    }

    return b
}